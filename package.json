{
  "name": "licenseplate",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "npm run start --workspace=licenseplate-mobile",
    "android": "npm run android --workspace=licenseplate-mobile",
    "ios": "npm run ios --workspace=licenseplate-mobile",
    "web": "npm run web --workspace=licenseplate-mobile",
    "deploy": "npm run deploy --workspace=licenseplate-mobile",
    "build:docs": "npm run build:docs --workspace=licenseplate-server",
    "server": "npm run dev --workspace=licenseplate-server",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "workspaces": [
    "apps/*"
  ],
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@react-native/eslint-plugin": "^0.80.2",
    "@react-native/typescript-config": "^0.80.2",
    "@types/react-native": "^0.72.8",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.0"
  },
  "dependencies": {
    "expo-camera": "~16.1.11"
  }
}
